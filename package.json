{
  "name": "chicfit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "ts-node --project tsconfig.json -r tsconfig-paths/register scripts/seed.ts",
    "seed-admin": "ts-node --project tsconfig.json -r tsconfig-paths/register scripts/seed-admin.ts"
  },
  "dependencies": {
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.19",
    "@mui/material": "5.14.19",
    "@sendgrid/mail": "8.1.0",
    "bcryptjs": "2.4.3",
    "date-fns": "2.30.0",
    "dotenv": "16.4.1",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.10.2",
    "next": "14.0.4",
    "next-auth": "4.24.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "stripe": "14.14.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/bcryptjs": "2.4.6",
    "@types/dotenv": "8.2.0",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "20.11.16",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended"
