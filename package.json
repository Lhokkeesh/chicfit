    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "overrides": {
    "glob": "10.3.10",
    "semver": "7.5.4",
    "@eslint/eslintrc": "3.0.0",
    "word-wrap": "1.2.5"
  },
  "resolutions": {
    "@types/react": "18.2.21",
    "glob": "10.3.10",
    "semver": "7.5.4"
  },
  "engines": {
    "node": ">=18.17.0"
  }
